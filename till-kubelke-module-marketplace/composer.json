{
    "name": "till-kubelke/module-marketplace",
    "description": "Marketplace Module - BGM Service Provider Directory and Inquiry System",
    "type": "symfony-bundle",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "doctrine/orm": "^3.0",
        "doctrine/collections": "^2.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/validator": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "TillKubelke\\ModuleMarketplace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TillKubelke\\ModuleMarketplace\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}

